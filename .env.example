# Optional user agent override (else Playwright default is used)
USER_AGENT=

# Telegram (optional)
# Create a bot via @BotFather and use /getid to retrieve the chat ID.
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# SendGrid (optional)
# Generate an API key and verified sender in the SendGrid dashboard.
SENDGRID_API_KEY=
SENDGRID_FROM=
SENDGRID_TO=

LOG_LEVEL=
HTTP_PROXY=
HEALTHCHECK_VERIFY=   # optional; set to "false" to disable TLS cert verification for healthcheck pings
