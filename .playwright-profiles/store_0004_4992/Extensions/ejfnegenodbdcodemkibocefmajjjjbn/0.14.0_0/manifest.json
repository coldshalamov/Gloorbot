   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Kapture MCP Browser Automation",
   "permissions": [ "activeTab", "debugger" ],
